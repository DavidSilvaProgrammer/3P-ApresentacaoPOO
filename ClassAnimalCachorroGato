class Animal:
    def emitir_som(self):
        print("Emitir som")
class Cachorro(Animal):
    def __init__(self):
        super().__init__()
    def emitir_som(self):
        print("Latir")
class Gato(Animal):
    def __init__(self):
        super().__init__()
    def emitir_som(self):
        print("Miar")

objeto_cachorro=Cachorro()
objeto_gato=Gato()
objeto_cachorro.emitir_som()
objeto_gato.emitir_som()
